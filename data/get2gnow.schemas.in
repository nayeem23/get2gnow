			<applyto>/apps/get2gnow/xml-cache/archive/replies</applyto>
			<owner>get2gnow</owner>
			<type>int</type>
			<default>604800</default>
			<locale name="C">
				<short>Seconds to keep retrieving replies since</short>
				<long>Seconds to keep retrieving replies since.  By default Replies, &amp; @ Mentions, from the last 14 days are loaded.</long>
			</locale>
		</schema>
		
