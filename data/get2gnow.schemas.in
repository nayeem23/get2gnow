			<applyto>/apps/get2gnow/xml-cache/archive/dms</applyto>
			<owner>get2gnow</owner>
			<type>int</type>
			<default>2419200</default>
			<locale name="C">
				<short>Seconds to keep retrieving DMs since</short>
				<long>Seconds to keep retrieving DMs since.  By default DMs from the last 4 weeks are loaded.</long>
			</locale>
		</schema>
		
