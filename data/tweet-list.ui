<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy toplevel-contextual -->
  <object class="GtkLabel" id="tweet_list_menu_label">
    <property name="visible">True</property>
    <property name="label" translatable="yes">tweet_list_menu_label</property>
    <property name="use_markup">True</property>
  </object>
  <object class="GtkAdjustment" id="tweet_list_max_tweets_adjustment">
    <property name="value">20</property>
    <property name="lower">20</property>
    <property name="upper">100</property>
    <property name="step_increment">10</property>
    <property name="page_increment">10</property>
  </object>
  <object class="GtkLabel" id="tweet_list_tab_label">
    <property name="visible">True</property>
    <property name="label" translatable="yes">tweet_list_tab_label</property>
    <property name="use_markup">True</property>
    <property name="use_underline">True</property>
  </object>
  <object class="GtkListStore" id="tweet_list_list_store">
    <columns>
      <!-- column-name ULONG_TWEET_ID -->
      <column type="gulong"/>
      <!-- column-name ULONG_USER_ID -->
      <column type="gulong"/>
      <!-- column-name STRING_USER -->
      <column type="gchararray"/>
      <!-- column-name STRING_NICK -->
      <column type="gchararray"/>
      <!-- column-name STRING_TEXT -->
      <column type="gchararray"/>
      <!-- column-name STRING_TWEET -->
      <column type="gchararray"/>
      <!-- column-name STRING_SEXY_TWEET -->
      <column type="gchararray"/>
      <!-- column-name STRING_CREATED_AGO -->
      <column type="gchararray"/>
      <!-- column-name STRING_CREATED_AT -->
      <column type="gchararray"/>
      <!-- column-name ULONG_CREATED_AGO -->
      <column type="gulong"/>
      <!-- column-name ULONG_CREATED_AT -->
      <column type="gulong"/>
      <!-- column-name PIXBUP_AVATAR -->
      <column type="GdkPixbuf"/>
      <!-- column-name ONLINE_SERVICE -->
      <column type="gpointer"/>
      <!-- column-name STRING_FROM -->
      <column type="gchararray"/>
      <!-- column-name STRING_RCPT -->
      <column type="gchararray"/>
    </columns>
  </object>
  <object class="GtkVBox" id="tweet_list_vbox">
    <property name="visible">True</property>
    <property name="orientation">vertical</property>
    <child>
      <object class="GtkHBox" id="tweet_list_hbox">
        <property name="visible">True</property>
        <child>
          <object class="GtkHandleBox" id="tweet_list_handlebox">
            <property name="visible">True</property>
            <child>
              <object class="GtkToolbar" id="tweet_list_toolbar">
                <property name="visible">True</property>
                <child>
                  <object class="GtkToolButton" id="tweet_list_refresh_tool_button">
                    <property name="visible">True</property>
                    <property name="label" translatable="yes">_Refresh</property>
                    <property name="use_underline">True</property>
                    <property name="stock_id">gtk-refresh</property>
                  </object>
                  <packing>
                    <property name="expand">False</property>
                    <property name="homogeneous">True</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkSeparatorToolItem" id="toolbutton2">
                    <property name="visible">True</property>
                  </object>
                  <packing>
                    <property name="expand">False</property>
                    <property name="homogeneous">True</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkToolItem" id="tweet_list_progress_bar_custom_tool_button">
                    <property name="visible">True</property>
                    <property name="border_width">12</property>
                    <child>
                      <object class="GtkProgressBar" id="tweet_list_progress_bar">
                        <property name="visible">True</property>
                        <property name="show_text">True</property>
                        <property name="adjustment">tweet_list_progress_bar_adjustment</property>
                        <property name="discrete_blocks">20</property>
                      </object>
                    </child>
                  </object>
                  <packing>
                    <property name="expand">True</property>
                    <property name="homogeneous">True</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkToolItem" id="tweet_list_max_tweets_custom_tool_button">
                    <property name="visible">True</property>
                    <child>
                      <object class="GtkFrame" id="tweet_list_max_tweets_frame">
                        <property name="visible">True</property>
                        <property name="label_xalign">0</property>
                        <property name="shadow_type">none</property>
                        <child>
                          <object class="GtkAlignment" id="tweet_list_max_tweets_alignment">
                            <property name="visible">True</property>
                            <property name="left_padding">12</property>
                            <child>
                              <object class="GtkSpinButton" id="tweet_list_max_tweets_spin_button">
                                <property name="visible">True</property>
                                <property name="can_focus">True</property>
                                <property name="max_length">100</property>
                                <property name="invisible_char">&#x25CF;</property>
                                <property name="shadow_type">etched-out</property>
                                <property name="adjustment">tweet_list_max_tweets_adjustment</property>
                                <property name="climb_rate">10</property>
                                <property name="snap_to_ticks">True</property>
                                <property name="numeric">True</property>
                                <property name="update_policy">if-valid</property>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child type="label">
                          <object class="GtkLabel" id="tweet_list_max_tweets_label">
                            <property name="visible">True</property>
                            <property name="label" translatable="yes">&lt;b&gt;Ma_x updates&lt;/b&gt;</property>
                            <property name="use_markup">True</property>
                            <property name="use_underline">True</property>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                  <packing>
                    <property name="expand">False</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkSeparatorToolItem" id="toolbutton1">
                    <property name="visible">True</property>
                  </object>
                  <packing>
                    <property name="expand">False</property>
                    <property name="homogeneous">True</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkToggleToolButton" id="tweet_list_stop_toggle_tool_button">
                    <property name="visible">True</property>
                    <property name="label" translatable="yes">_Stop</property>
                    <property name="use_underline">True</property>
                    <property name="stock_id">gtk-stop</property>
                  </object>
                  <packing>
                    <property name="expand">False</property>
                    <property name="homogeneous">True</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkToolButton" id="tweet_list_close_tool_button">
                    <property name="visible">True</property>
                    <property name="label" translatable="yes">_Close</property>
                    <property name="use_underline">True</property>
                    <property name="stock_id">gtk-close</property>
                  </object>
                  <packing>
                    <property name="expand">False</property>
                    <property name="homogeneous">True</property>
                  </packing>
                </child>
              </object>
            </child>
          </object>
          <packing>
            <property name="position">0</property>
          </packing>
        </child>
      </object>
      <packing>
        <property name="expand">False</property>
        <property name="fill">False</property>
        <property name="position">0</property>
      </packing>
    </child>
    <child>
      <object class="GtkHSeparator" id="tweet_list_hseparator">
        <property name="visible">True</property>
      </object>
      <packing>
        <property name="expand">False</property>
        <property name="fill">False</property>
        <property name="position">1</property>
      </packing>
    </child>
    <child>
      <object class="GtkScrolledWindow" id="tweet_list_scrolled_window">
        <property name="visible">True</property>
        <property name="can_focus">True</property>
        <property name="hscrollbar_policy">automatic</property>
        <property name="vscrollbar_policy">automatic</property>
        <child>
          <object class="GtkTreeView" id="tweet_list_timeline_tree_view">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="model">tweet_list_list_store</property>
            <property name="headers_clickable">False</property>
            <property name="expander_column">tweet_list_string_tweet_tree_view_column</property>
            <property name="reorderable">True</property>
            <property name="rules_hint">True</property>
            <property name="search_column">1</property>
            <property name="level_indentation">5</property>
            <property name="enable_grid_lines">both</property>
            <property name="enable_tree_lines">True</property>
            <property name="tooltip_column">6</property>
            <child>
              <object class="GtkTreeViewColumn" id="tweet_list_pixbuf_avatar_tree_view_column">
                <property name="sizing">fixed</property>
                <property name="fixed_width">58</property>
                <property name="min_width">58</property>
                <property name="max_width">58</property>
                <property name="title">Avatar</property>
                <child>
                  <object class="GtkCellRendererPixbuf" id="tweet_list_pixbuf_avatar_cell_renderer_pixbuf">
                    <property name="xpad">5</property>
                    <property name="ypad">5</property>
                  </object>
                  <attributes>
                    <attribute name="pixbuf">11</attribute>
                  </attributes>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkTreeViewColumn" id="tweet_list_string_tweet_tree_view_column">
                <property name="sizing">fixed</property>
                <property name="title">Update</property>
                <property name="expand">True</property>
                <property name="clickable">True</property>
                <child>
                  <object class="GtkCellRendererText" id="tweet_list_string_tweet_cell_renderer_text">
                    <property name="xpad">5</property>
                    <property name="yalign">0</property>
                    <property name="editable">True</property>
                    <property name="rise">13</property>
                    <property name="stretch">semi-condensed</property>
                    <property name="wrap_mode">word-char</property>
                    <property name="wrap_width">200</property>
                  </object>
                  <attributes>
                    <attribute name="markup">5</attribute>
                  </attributes>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkTreeViewColumn" id="tweet_list_string_from_tree_view_column">
                <property name="sizing">fixed</property>
                <property name="fixed_width">124</property>
                <property name="min_width">60</property>
                <property name="max_width">124</property>
                <property name="title">From:</property>
                <child>
                  <object class="GtkCellRendererText" id="tweet_list_string_from_cell_renderer_text">
                    <property name="yalign">0</property>
                    <property name="alignment">right</property>
                    <property name="stretch">extra-condensed</property>
                    <property name="variant">small-caps</property>
                    <property name="wrap_mode">word-char</property>
                    <property name="wrap_width">124</property>
                  </object>
                  <attributes>
                    <attribute name="markup">13</attribute>
                  </attributes>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkTreeViewColumn" id="tweet_list_string_rcpt_tree_view_column">
                <property name="sizing">fixed</property>
                <property name="fixed_width">124</property>
                <property name="min_width">60</property>
                <property name="max_width">124</property>
                <property name="title">RCPT:</property>
                <child>
                  <object class="GtkCellRendererText" id="tweet_list_string_rcpt_cell_renderer_text">
                    <property name="yalign">0</property>
                    <property name="alignment">right</property>
                    <property name="stretch">extra-condensed</property>
                    <property name="wrap_mode">word-char</property>
                    <property name="wrap_width">124</property>
                  </object>
                  <attributes>
                    <attribute name="markup">14</attribute>
                  </attributes>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkTreeViewColumn" id="tweet_list_created_ago_str_tree_view_column">
                <property name="sizing">fixed</property>
                <property name="fixed_width">112</property>
                <property name="max_width">115</property>
                <property name="title">Received</property>
                <child>
                  <object class="GtkCellRendererText" id="tweet_list_created_ago_str_cell_renderer_text">
                    <property name="yalign">0</property>
                    <property name="alignment">right</property>
                    <property name="stretch">condensed</property>
                    <property name="style">italic</property>
                    <property name="width_chars">15</property>
                    <property name="wrap_mode">word-char</property>
                  </object>
                  <attributes>
                    <attribute name="text">7</attribute>
                  </attributes>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
      <packing>
        <property name="position">2</property>
      </packing>
    </child>
  </object>
  <object class="GtkAdjustment" id="tweet_list_progress_bar_adjustment">
    <property name="upper">100</property>
    <property name="step_increment">10</property>
    <property name="page_increment">10</property>
    <property name="page_size">10</property>
  </object>
</interface>
