<?xml version="1.0"?>
<interface>
  <!-- interface-requires gtk+ 2.12 -->
  <!-- interface-naming-policy toplevel-contextual -->
  <object class="GtkTreeStore" id="friends_manager_tree_store">
    <columns>
      <!-- column-name GSTRING_USER_NAME -->
      <column type="gchararray"/>
      <!-- column-name GSTRING_USER_NICK -->
      <column type="gchararray"/>
      <!-- column-name GSTRING_FOLLOWING -->
      <column type="gchararray"/>
      <!-- column-name GSTRING_FOLLOWER -->
      <column type="gchararray"/>
      <!-- column-name GSTRING_CREATED_AGO -->
      <column type="gchararray"/>
      <!-- column-name USER_POINTER -->
      <column type="gpointer"/>
    </columns>
  </object>
  <object class="GtkDialog" id="friends_manager">
    <property name="width_request">780</property>
    <property name="height_request">300</property>
    <property name="visible">True</property>
    <property name="title" translatable="yes">Friends Manager</property>
    <property name="window_position">center-on-parent</property>
    <property name="default_width">775</property>
    <property name="default_height">425</property>
    <property name="destroy_with_parent">True</property>
    <property name="type_hint">dialog</property>
    <child internal-child="vbox">
      <object class="GtkVBox" id="dialog-vbox5">
        <property name="visible">True</property>
        <child>
          <object class="GtkNotebook" id="friends_manager_notebook">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <child>
              <object class="GtkVBox" id="friends_manager_vbox">
                <property name="visible">True</property>
                <property name="orientation">vertical</property>
                <child>
                  <object class="GtkScrolledWindow" id="friends_manager_scrolled_window">
                    <property name="visible">True</property>
                    <property name="can_focus">True</property>
                    <property name="border_width">6</property>
                    <property name="hscrollbar_policy">never</property>
                    <property name="shadow_type">in</property>
                    <child>
                      <object class="GtkTreeView" id="friends_and_followers">
                        <property name="visible">True</property>
                        <property name="can_focus">True</property>
                        <property name="model">friends_manager_tree_store</property>
                        <property name="headers_clickable">False</property>
                        <property name="search_column">1</property>
                        <property name="show_expanders">False</property>
                        <child>
                          <object class="GtkTreeViewColumn" id="friends_manager_user_name_tree_view_column">
                            <property name="resizable">True</property>
                            <property name="title">User</property>
                            <child>
                              <object class="GtkCellRendererText" id="friends_manager_user_name_tree_cell_renderer_text"/>
                              <attributes>
                                <attribute name="text">0</attribute>
                              </attributes>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkTreeViewColumn" id="friends_manager_user_nick_name_tree_view_column">
                            <property name="resizable">True</property>
                            <property name="title">Name</property>
                            <child>
                              <object class="GtkCellRendererText" id="friends_manager_user_nick_name_cell_renderer_text"/>
                              <attributes>
                                <attribute name="text">1</attribute>
                              </attributes>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkTreeViewColumn" id="friends_manager_friends_tree_view_column">
                            <property name="resizable">True</property>
                            <property name="title">Am I following them?</property>
                            <child>
                              <object class="GtkCellRendererText" id="friends_manager_friends_cell_renderer_text"/>
                              <attributes>
                                <attribute name="text">2</attribute>
                              </attributes>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkTreeViewColumn" id="friends_manager_follower_tree_view_column">
                            <property name="resizable">True</property>
                            <property name="title">Are they following me?</property>
                            <child>
                              <object class="GtkCellRendererText" id="friends_manager_follower_cell_renderer_text"/>
                              <attributes>
                                <attribute name="text">3</attribute>
                              </attributes>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkTreeViewColumn" id="friends_manager_created_age_tree_view_column">
                            <property name="title">Last Updated:</property>
                            <child>
                              <object class="GtkCellRendererText" id="friends_manager_created_age_cell_renderer_text"/>
                              <attributes>
                                <attribute name="text">4</attribute>
                              </attributes>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                  <packing>
                    <property name="position">0</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkHBox" id="hbox2">
                    <property name="visible">True</property>
                    <property name="spacing">5</property>
                    <child>
                      <object class="GtkButton" id="refresh">
                        <property name="label">gtk-refresh</property>
                        <property name="visible">True</property>
                        <property name="can_focus">True</property>
                        <property name="receives_default">True</property>
                        <property name="use_stock">True</property>
                      </object>
                      <packing>
                        <property name="expand">False</property>
                        <property name="fill">False</property>
                        <property name="position">0</property>
                      </packing>
                    </child>
                    <child>
                      <object class="GtkButton" id="user_follow">
                        <property name="label" translatable="yes">_Follow</property>
                        <property name="visible">True</property>
                        <property name="sensitive">False</property>
                        <property name="can_focus">True</property>
                        <property name="receives_default">True</property>
                        <property name="image">follow_image</property>
                        <property name="use_underline">True</property>
                      </object>
                      <packing>
                        <property name="expand">False</property>
                        <property name="fill">False</property>
                        <property name="padding">5</property>
                        <property name="position">1</property>
                      </packing>
                    </child>
                    <child>
                      <object class="GtkButton" id="user_unfollow">
                        <property name="label" translatable="yes">_Unfollow</property>
                        <property name="visible">True</property>
                        <property name="can_focus">True</property>
                        <property name="receives_default">True</property>
                        <property name="image">unfollow_image</property>
                        <property name="use_underline">True</property>
                      </object>
                      <packing>
                        <property name="expand">False</property>
                        <property name="fill">False</property>
                        <property name="padding">5</property>
                        <property name="position">2</property>
                      </packing>
                    </child>
                    <child>
                      <object class="GtkButton" id="user_block">
                        <property name="label" translatable="yes">_Block</property>
                        <property name="visible">True</property>
                        <property name="sensitive">False</property>
                        <property name="can_focus">True</property>
                        <property name="receives_default">True</property>
                        <property name="image">block_image</property>
                        <property name="use_underline">True</property>
                      </object>
                      <packing>
                        <property name="expand">False</property>
                        <property name="fill">False</property>
                        <property name="padding">6</property>
                        <property name="position">3</property>
                      </packing>
                    </child>
                    <child>
                      <object class="GtkButton" id="view_profile">
                        <property name="label" translatable="yes">_Profile</property>
                        <property name="visible">True</property>
                        <property name="can_focus">True</property>
                        <property name="receives_default">True</property>
                        <property name="image">view_profile_image</property>
                        <property name="use_underline">True</property>
                      </object>
                      <packing>
                        <property name="expand">False</property>
                        <property name="fill">False</property>
                        <property name="position">4</property>
                      </packing>
                    </child>
                    <child>
                      <object class="GtkButton" id="view_timeline">
                        <property name="label" translatable="yes">_Tweets/Updates</property>
                        <property name="visible">True</property>
                        <property name="can_focus">True</property>
                        <property name="receives_default">True</property>
                        <property name="image">view_timeline_image</property>
                        <property name="use_underline">True</property>
                      </object>
                      <packing>
                        <property name="expand">False</property>
                        <property name="fill">False</property>
                        <property name="padding">5</property>
                        <property name="position">5</property>
                      </packing>
                    </child>
                  </object>
                  <packing>
                    <property name="expand">False</property>
                    <property name="padding">4</property>
                    <property name="pack_type">end</property>
                    <property name="position">1</property>
                  </packing>
                </child>
              </object>
            </child>
            <child type="tab">
              <object class="GtkLabel" id="friends_manager_notebook_tab_label">
                <property name="visible">True</property>
                <property name="label" translatable="yes">_Manage Friends &amp; Followers</property>
                <property name="use_underline">True</property>
                <property name="mnemonic_widget">friends_and_followers</property>
              </object>
              <packing>
                <property name="tab_fill">False</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="position">2</property>
          </packing>
        </child>
        <child internal-child="action_area">
          <object class="GtkHButtonBox" id="dialog-action_area5">
            <property name="visible">True</property>
            <property name="layout_style">end</property>
            <child>
              <object class="GtkButton" id="lists_close">
                <property name="label">gtk-close</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
                <property name="use_stock">True</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">False</property>
                <property name="position">0</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="pack_type">end</property>
            <property name="position">0</property>
          </packing>
        </child>
      </object>
    </child>
    <action-widgets>
      <action-widget response="-5">lists_close</action-widget>
    </action-widgets>
  </object>
  <object class="GtkImage" id="view_timeline_image1">
    <property name="visible">True</property>
    <property name="stock">gtk-home</property>
  </object>
  <object class="GtkImage" id="follow_image">
    <property name="visible">True</property>
    <property name="stock">gtk-add</property>
  </object>
  <object class="GtkImage" id="unfollow_image">
    <property name="visible">True</property>
    <property name="stock">gtk-refresh</property>
  </object>
  <object class="GtkImage" id="block_image">
    <property name="width_request">24</property>
    <property name="height_request">24</property>
    <property name="visible">True</property>
    <property name="tooltip_text" translatable="yes">Block this user from being able to see your tweets or send you messages.</property>
    <property name="stock">gtk-stop</property>
  </object>
  <object class="GtkImage" id="view_profile_image">
    <property name="visible">True</property>
    <property name="stock">gtk-info</property>
  </object>
  <object class="GtkImage" id="view_timeline_image">
    <property name="visible">True</property>
    <property name="stock">gtk-home</property>
  </object>
</interface>
