#!/bin/tcsh -f
set my_editor = "`printf "\""${0}"\"" | sed 's/.*\/\([^\:]\+\).*/\1/g'`"

if("${my_editor}" == "${0}" ) then
	echo "**error:** unable to determine session editor
your default editor will be used." > /dev/stderr;
	set my_editor="${EDITOR}";
endif

switch ( "${my_editor}" )
case "connectED":
case "gedit":
	breaksw
case "vi":
case "vim":
case "vim-enhanced":
default:
	set my_editor = `printf "%s -p" "vim-enhanced"`
	breaksw
endsw

${my_editor} "/projects/gtk/get2gnow/src/main-window.c" "/projects/gtk/get2gnow/src/online-service.c" "/projects/gtk/get2gnow/src/online-service.h" "/projects/gtk/get2gnow/src/online-services.c" "/projects/gtk/get2gnow/src/online-services-dialog.c" "/projects/gtk/get2gnow/src/online-services.h" "/projects/gtk/get2gnow/src/online-service.types.h" "/projects/gtk/get2gnow/src/update-viewer.c"

