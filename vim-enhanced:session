#!/bin/tcsh -f
set my_editor = "`printf "\""${0}"\"" | sed 's/.*\/\([^\.]\+\).*/\1/g'`"
switch ( "${my_editor}" )
case "connectED":
case "gedit":
	breaksw
case "vi":
case "vim":
case "vim-enhanced":
default:
	set my_editor = `printf "%s -p" "vim-enhanced"`
	breaksw
endsw

${my_editor} "./bugreport.txt" "./src/cache.c" "./src/online-service.c" "./src/online-service-request.c" "./src/online-service-wrapper.c" "./src/online-service-wrapper.h" "./src/tweet-list.c" "./src/users.c"
