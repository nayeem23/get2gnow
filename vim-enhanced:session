#!/bin/tcsh -f
set my_editor = "`printf "\""${0}"\"" | sed 's/.*\/\([^\.]\+\).*/\1/g'`"
switch ( "${my_editor}" )
case "connectED":
case "gedit":
	breaksw
case "vi":
case "vim":
case "vim-enhanced":
default:
	set my_editor = `printf "%s -p" "vim-enhanced"`
	breaksw
endsw

${my_editor} "./configure.in" "./configure" "./data/get2gnow.schemas.in" "./src/label.c" "./src/network.c" "./src/online-services-typedefs.h" "./src/parser.c" "./src/preferences.c" "./src/preferences.h" "./src/tweet-list.c" "./src/tweet-view.c" "./src/users.c"
